{
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["vite/client", "vite-plugin-svgr/client"],
    "esModuleInterop": true,
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "moduleResolution": "bundler",
    "module": "ESNext",
    "skipLibCheck": true
  },
  "include": ["src"]
}
